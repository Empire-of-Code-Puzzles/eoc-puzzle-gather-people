if __name__ == '__main__':
    # These "asserts" using only for self-checking and not necessary for auto-testing
    assert evacuate([[0, 40, 40, 0, 0, 0],
                     [40, 6, 0, 40, 0, 0],
                     [40, 0, 4, 40, 0, 20],
                     [0, 40, 40, 1, 20, 0],
                     [0, 0, 0, 20, 3, 0],
                     [0, 0, 20, 0, 0, 2]], 80) == 81, "Example"
    assert evacuate([[0, 0, 40, 0, 40, 40, 0, 40, 0],
                     [0, 1, 40, 0, 40, 0, 0, 0, 0],
                     [40, 40, 1, 40, 0, 0, 0, 0, 0],
                     [0, 0, 40, 1, 0, 40, 0, 0, 0],
                     [40, 40, 0, 0, 1, 0, 40, 0, 0],
                     [40, 0, 0, 40, 0, 1, 0, 0, 40],
                     [0, 0, 0, 0, 40, 0, 1, 40, 0],
                     [40, 0, 0, 0, 0, 0, 40, 1, 40],
                     [0, 0, 0, 0, 0, 40, 0, 40, 1]],
                    1000) == 100, "Everybody"
    assert evacuate([[0, 40, 0, 40, 0, 0],
                     [40, 6, 0, 0, 40, 0],
                     [0, 0, 3, 0, 28, 0],
                     [40, 0, 0, 4, 40, 28],
